{
  "name": "tustin/psn-php",
  "license": "MIT",
  "description": "PHP wrapper for the PlayStation API.",
  "homepage": "https://github.com/Tustin/psn-php",
  "keywords": [
    "playstation",
    "api",
    "php"
  ],
  "autoload": {
    "psr-4": {
      "Tustin\\PlayStation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "guzzlehttp/guzzle": "^7.8",
    "php": "^8.2",
    "tustin/haste": "^1.1",
    "nesbot/carbon": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "pestphp/pest": "^1.23"
  },
  "scripts": {
    "phpunit": "composer dump-autoload && vendor/bin/phpunit"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
